"EyeRefract"
{
	"$Iris"               "models/cra0kalo/shared/eyes_japan/adien_jem" 		  // Iris color in RGB with cornea noise in A
	"$Irisframe" "0"				// Frame for the iris texture, 0 being the default frame
	"$AmbientOcclTexture" "models/cra0kalo/shared/eyes_japan/eye_ao2"				  // Ambient occlusion in RGB, A unused // default texture eyeball_l_ambient 
	"$Envmap"             "models/cra0kalo/shared/eyes_japan/blueglint"    // Reflection environment map static
	//"$Envmap"             "models/cra0kalo/shared/eyes_japan/eye-reflection-cubemap-"    // Reflection environment map
	"$CorneaTexture"      "models/cra0kalo/shared/eyes_japan/eye-cornea"                 // Special texture that has 2D cornea normal in RG and other data in BA
	//"$lightwarptexture" "models/cra0kalo/alisa/eyes_japan/eye_lightwarp"		// 1D ramp texture for tinting scalar diffuse term
		"$lightwarptexture"   "models/cra0kalo/shared/eyes_japan/eye_lightwarp"  // This enables TF NPR lighting

	"$EyeballRadius" "0.5"				// Default 0.5
	"$AmbientOcclColor" "[0.36 0.25 0.2]"	// Default 0.33, 0.33, 0.33
	"$Dilation" "0.4"					// Default 0.5
	"$Glossiness" "1"					// Glossiness of eye (1 is default, 0 is not glossy at all)
	"$ParallaxStrength" "0.10"          // Default 0.25
	"$CorneaBumpStrength" "1.0"			// Default 1.0

	"$halflambert" "1"
	"$nodecal" "1"

	// These effects are only available in ps.2.0b and later
	"$RaytraceSphere" "0"				// Default 1 - Enables raytracing in the pixel shader to make the eyeball look round
	"$SphereTexkillCombo" "0"			// Default 1 - Enables killing pixels that don't ray-intersect the sphere


	//Unknown Parameters
	

	"$eyeorigin" "[0 0 0]" // Origin for the eyes
	"$irisu" "[0 1 0 0]"
	"$irisv" "[0 0 1 0]"
	"$Entityorigin" "4.0"


}
