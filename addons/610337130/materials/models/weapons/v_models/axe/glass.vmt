"UnlitGeneric"
{
	"$basetexture"  "models/Weapons/v_models\axe\glass"
	"$texture2"	"models/Weapons/v_models/axe/noise"       
	"$frame" "3"
	"$additive" 1
	"$model" 1
	//"$envmap" "env_cubemap"
	//"$envmaptint" "[ 0.6 0.6 0.6 ]"
	"Proxies" 
	{
		"AnimatedTexture" 
		{ 
			"animatedTextureVar" "$basetexture" 
			"animatedTextureFrameNumVar" "$frame" 
			"animatedTextureFrameRate" "21" //this is the FPS that the texture will animate at
		}
	
	}
	"Proxies" 
	{
		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" 3
			"texturescrollangle" -50
		}


	}
}



